<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
	"http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
	<servlet>
		<servlet-name>Context</servlet-name>
		<servlet-class>us.mn.state.dot.video.server.PropertiesContext</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet>
		<servlet-name>clip</servlet-name>
		<servlet-class>us.mn.state.dot.video.server.nvr.ClipServer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>clip</servlet-name>
		<url-pattern>/clip</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>range</servlet-name>
		<servlet-class>us.mn.state.dot.video.server.nvr.RangeServer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>range</servlet-name>
		<url-pattern>/range</url-pattern>
	</servlet-mapping>
</web-app>
